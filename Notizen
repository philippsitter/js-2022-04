// Bedingung
let zahl = 0
while(zahl < 5) {
    console.debug("Hallo")
    zahl++
}
//Dasselbe:
//Variabledeklarieren/Anfangswert; Bedingung wie lange; Wie schnell zählen wir hoch
for (let i=0; i<5;i++) {
    console.debug("Hallo")
}

for(let i=0; i <10; i++){
    console.debug("Hallo wie gehts dir?")
}

for(let i=0; i <10; i++)
    //1.durchlauf i=0 // 1.Schritt
    //2.durchlauf i=0 // .Schritt
    for(let j = 0; j < 3; j++){  ----------------screenshot 15:23 ca 20.05
        console.debug("hallo")
    }
}


